{
  "name": "vue-autoheight-input",
  "version": "0.1.1",
  "private": false,
  "main": "dist/vue-autoheight-input.common.js",
  "unpkg": "dist/vue-autoheight-input.umd.min.js",
  "jsdelivr": "dist/vue-autoheight-input.umd.min.js",
  "author": "Dilan Tsasi",
  "license": "MIT",
  "description": "A simple vuejs plugin for auto resizing the height of a textarea",
  "files": [
    "dist/*"
  ],
  "homepage": "https://github.com/DilanTsasi/vue-autoheight-input",
  "repository": {
    "type": "git",
    "url": "https://github.com/DilanTsasi/vue-autoheight-input.git"
  },
  "bugs": {
    "url": "https://github.com/DilanTsasi/vue-autoheight-input/issues"
  },
  "keywords": [
    "vue",
    "wrapper",
    "editor",
    "autosize input",
    "textarea"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-library": "vue-cli-service build --target lib --name vue-autoheight-input src/main.js",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "node \"%npm_execpath%\" build"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
